<Page x:Class="clientDB.SigningUpPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:clientDB"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="SigningUpPage">

    <Grid>
        <Button x:Name="buttonSignUp" Content="Зарегестрироваться" HorizontalAlignment="Center" Margin="44,89,46,0" VerticalAlignment="Top" Width="210" Grid.ColumnSpan="2" Height="20" Click="buttonSignUp_Click"/>
        <PasswordBox x:Name="passwordBox" Margin="44,59,46,0" VerticalAlignment="Top" Height="25" HorizontalAlignment="Center" Width="210" Grid.ColumnSpan="2"/>
        <TextBox x:Name="textBoxLogin" HorizontalAlignment="Center" Height="23" Margin="44,31,46,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="210" Grid.ColumnSpan="2"/>
        <TextBlock x:Name="textBlockHat" HorizontalAlignment="Center" Margin="117,10,117,0" TextWrapping="Wrap" Text="чё написать" VerticalAlignment="Top" Grid.ColumnSpan="2" Height="16" Width="66"/>
    </Grid>
</Page>
